{
  "name": "electron-create-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cross-env": "^5.0.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "electron": "^1.6.10",
    "electron-devtools-installer": "^2.2.0",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "debug-main": "node_modules/.bin/electron-inspector",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/electron ./main.js",
    "start": "cross-env NODE_ENV=production ./node_modules/.bin/electron ./main.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "homepage": "./"
}
